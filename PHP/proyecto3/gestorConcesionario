<?php

    include "Coche.php";

    function addCoche(){
        if(isset($_GET["matricula"]) && isset($_GET["marca"]) && isset($_GET["modelo"]) && isset($_GET["km"])){
            $matricula = $_GET['matricula'];
            $marca = $_GET['marca']; 
            $modelo = $_GET['modelo']; 
            $km = $_GET['km']; 

            return new Coche($matricula, $marca, $modelo, $km);
        }
    }

    
?>